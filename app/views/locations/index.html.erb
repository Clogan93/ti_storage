<%= render partial: 'common/breadcrumbs', locals: {
  links: [
    params[:area_slug] ? link_to(@storages.first.area_name, "/#{params[:area_slug]}") : link_to('Locations',  locations_path)
  ]
} %>

<main id="locations_page">
  <div class="container-fluid">

    <% @storages.each do |storage| %>
      <%= render(partial: 'common/storage_view', locals: { storage: storage }) %>
    <% end %>

  </div>
</main>